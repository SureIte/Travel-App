                </androidx.cardview.widget.CardView>

                <TextView
                    android:id="@+id/textView23"
                    android:layout_width="61dp"
                    android:layout_height="24dp"
                    android:layout_marginTop="6dp"
                    android:paddingStart="14dp"
                    android:text="Map"
                    app:layout_constraintBottom_toBottomOf="@+id/textView25"
                    app:layout_constraintEnd_toEndOf="@+id/imageView35"
                    app:layout_constraintStart_toStartOf="@+id/imageView35"
                    app:layout_constraintTop_toBottomOf="@+id/imageView35" />


                <androidx.cardview.widget.CardView
                    android:id="@+id/imageViewHotel"
                    android:layout_width="70dp"
                    android:layout_height="50dp"
                    android:layout_marginStart="16dp"
                    app:layout_constraintStart_toEndOf="@+id/imageView35"
                    app:layout_constraintTop_toTopOf="parent"
                    tools:ignore="DuplicateIds,MissingConstraints">


                    <ImageView
                        android:id="@+id/imageView37"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        app:srcCompat="@drawable/img_hotel_24" />
                </androidx.cardview.widget.CardView>

                <TextView
                    android:id="@+id/textView24"
                    android:layout_width="61dp"
                    android:layout_height="24dp"
                    android:layout_marginTop="6dp"
                    android:paddingStart="14dp"
                    android:text="Hotel"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="@+id/imageViewHotel"
                    app:layout_constraintStart_toStartOf="@+id/imageViewHotel"
                    app:layout_constraintTop_toBottomOf="@+id/imageViewHotel" />

                <androidx.cardview.widget.CardView
